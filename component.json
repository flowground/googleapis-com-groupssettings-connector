{
    "title": "Groups Settings",
    "description": "Lets you manage permission levels and related settings of a group.",
    "docsUrl": "https://developers.google.com/google-apps/groups-settings/get_started",
    "url": "https://api.apis.guru/v2/specs/googleapis.com/groupssettings/v1/swagger.json",
    "envVars": {
        "OAUTH_CLIENT_ID": {
            "description": "OAuth Client ID"
        },
        "OAUTH_CLIENT_SECRET": {
            "description": "OAuth Client Secret"
        }
    },
    "credentials": {
        "fields": {
            "server": {
                "label": "Server",
                "viewClass": "SelectView",
                "model": [
                    "https://www.googleapis.com/groups/v1/groups",
                    "--- Custom URL"
                ],
                "required": true
            },
            "otherServer": {
                "label": "Custom Server URL",
                "viewClass": "TextFieldView"
            },
            "Oauth2": {
                "label": "Oauth 2.0 authentication",
                "viewClass": "OAuthFieldView"
            }
        },
        "oauth2": {
            "client_id": "{{OAUTH_CLIENT_ID}}",
            "client_secret": "{{OAUTH_CLIENT_SECRET}}",
            "auth_uri": "https://accounts.google.com/o/oauth2/auth"
        }
    },
    "triggers": {
        "startFlow": {
            "main": "./lib/triggers/startFlow.js",
            "type": "polling",
            "title": "Start Flow",
            "fields": {}
        }
    },
    "actions": {
        "groupsSettings_groups_get": {
            "main": "./lib/actions/groupsSettings_groups_get.js",
            "title": "Gets one resource by id.",
            "description": "Gets one resource by id.",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/groupsSettings_groups_get.in.json",
                "out": {
                    "type": "object"
                }
            }
        },
        "groupsSettings_groups_patch": {
            "main": "./lib/actions/groupsSettings_groups_patch.js",
            "title": "Updates an existing resource. This method supports patch semantics.",
            "description": "Updates an existing resource. This method supports patch semantics.",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/groupsSettings_groups_patch.in.json",
                "out": {
                    "type": "object"
                }
            }
        },
        "groupsSettings_groups_update": {
            "main": "./lib/actions/groupsSettings_groups_update.js",
            "title": "Updates an existing resource.",
            "description": "Updates an existing resource.",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/groupsSettings_groups_update.in.json",
                "out": {
                    "type": "object"
                }
            }
        }
    }
}